
{
  "com.badlogic.gdx.graphics.g2d.BitmapFont": {
    "default-font": {
      "file": "fonts/quarmic-sans-abridged-32.fnt"
    },
    "large-font": {
      "file": "fonts/quarmic-sans-abridged-64.fnt"
    }
  },

  "com.badlogic.gdx.graphics.Color": {
    "white": {
      "a": 1,
      "b": 1,
      "g": 1,
      "r": 1
    },
    "black": {
      "a": 1,
      "b": 0,
      "g": 0,
      "r": 0
    },
    "gray": {
      "a": 1,
      "b": 0.5,
      "g": 0.5,
      "r": 0.5
    }
  },

  "com.badlogic.gdx.scenes.scene2d.ui.TextButton$TextButtonStyle": {
    "default": {
      "down": "button-pressed",
      "up": "button-default",
      "font": "default-font",
      "fontColor": "white",
      "downFontColor": "black",
      "overFontColor": "gray"
    },
    "locked": {
      "down": "button-locked-pressed",
      "up": "button-locked",
      "font": "default-font",
      "fontColor": "white"
    }
  },

  "com.badlogic.gdx.scenes.scene2d.ui.Button$ButtonStyle": {
    "repeat": {
      "down": "small_button_repeat-pressed",
      "up": "small_button_repeat"
    },
    "ad": {
      "down": "small_button_ad-pressed",
      "up": "small_button_ad"
    },
    "home": {
      "down": "small_button_home-pressed",
      "up": "small_button_home"
    },
    "next": {
      "down": "small_button_next-pressed",
      "up": "small_button_next"
    }
  },

  "com.badlogic.gdx.scenes.scene2d.ui.Label$LabelStyle": {
    "default": {
      "font": "large-font"
    },
    "small": {
      "font": "default-font",
      "fontColor": "black"
    }
  },

  "com.badlogic.gdx.scenes.scene2d.ui.CheckBox$CheckBoxStyle": {
    "default": {
      "down": "button-pressed",
      "up": "button-default",
      "checkboxOn": "checkbox_tick",
      "checkboxOff": "checkbox",
      "font": "default-font",
      "fontColor": "white",
      "downFontColor": "black",
      "overFontColor": "gray"
    }
  },

  "com.badlogic.gdx.scenes.scene2d.ui.Window$WindowStyle": {
    "default": {
      "background": "label",
      "titleFont": "default-font"
    }
  }
}